  "maintainers": {
    "Howard Wu": "howardwu@berkeley.edu",
    "Andrew Malta": "andrew.malta@yale.edu"
  },
  "dependencies": {
    "chart.js": "^1.0.2",
    "express": "^4.13.3",
    "react": "^0.13.3",
    "react-bootstrap": "^0.24.3",
    "react-chartjs": "^0.6.0",
    "xhr": "^2.0.3"
  },
  "devDependencies": {
